@article{ WOS:A1983RV43100007,
Author = {GLOTOV, NV and GRITZENKO, VV},
Title = {AN ECOLOGICAL-GENETICAL STUDY OF FESTUCA-WORONOWI HACK IN DAGHESTAN},
Journal = {ZHURNAL OBSHCHEI BIOLOGII},
Year = {1983},
Volume = {44},
Number = {6},
Pages = {823-830},
Abstract = {The ecological-genetical structure of the variation of quantitative
   characters was studied in natural population of F. woronowi in
   Daghestan. Experimental cloning of the plants in different ecological
   conditions was realized, as well as familial sowings in geographically
   remote regions and express-test of variation with familial germination
   of seeds on extreme ecological backgrounds. The largest amount of
   variation is determined by ecological factors, but an important
   contribution is due to genetic factors. A significant interaction of the
   families with environmental conditions reflected the diversity of
   genotypes by their reaction to different conditions. The environmental
   reactions were compared with the genetic effects of the families per se.
   Some differences between populations were noted. Among the methods used
   for estimating variation, the express-test was the most efficient. The
   ecological-genetic variation was expressed most fully on extreme
   ecological backgrounds.},
