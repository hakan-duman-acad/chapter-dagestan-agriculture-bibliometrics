@article{ WOS:000773952600011,
Author = {Semenova, V. V. and Anatov, D. M.},
Title = {Accumulation of Heavy Metals in Plants of the Genus <i>Achillea</i> L.
   under Arid Conditions of the Plain Zone of Dagestan},
Journal = {ARID ECOSYSTEMS},
Year = {2022},
Volume = {12},
Number = {1},
Pages = {85-92},
Month = {MAR},
Abstract = {Arid, herb-grass steppes are widespread in the plain zone of Dagestan.
   Their herbage contains yarrow plants that form vast thickets in the
   lowland. The accumulation of heavy metals in organs of plants of the
   genus Achillea L. and in soils under the conditions of Dagestan has been
   studied for the first time. The concentrations of Fe, Mn, Pb, and Cd
   have been determined in the aboveground and underground mass of plants
   of the genus Achillea L. from the background and anthropogenically
   disturbed areas, as well as in the soils on which they grow. It is found
   that the accumulation and distribution of heavy metals in plant organs
   is influenced by vehicle pollution. In Achillea nobilis plants selected
   from the villages of Uchkent and Stalskoe, the cadmium concentration
   exceeds the maximum permissible level by 1.2-1.7 times. The calculated
