@article{ WOS:000179372500012,
Author = {Bulayeva, KB and Pavlova, TA and Kurbanov, RM and Bulayev, OA},
Title = {Mapping genes of complex disease in genetic isolates of Daghestan},
Journal = {RUSSIAN JOURNAL OF GENETICS},
Year = {2002},
Volume = {38},
