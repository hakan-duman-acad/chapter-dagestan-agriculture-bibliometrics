@inproceedings{ WOS:000629964000014,
Author = {Anatov, Dzhalaludin M.},
Editor = {Egorov, E and Ilina, I and Zaporozhets, N},
Title = {Some morphological traits of the fruit and apricot stone of Dagestan
   origin at the different ripening periods},
Booktitle = {INTERNATIONAL SCIENTIFIC ONLINE-CONFERENCE BIOENGINEERING IN THE
   ORGANIZATION OF PROCESSES CONCERNING BREEDING AND REPRODUCTION OF
   PERENNIAL CROPS 2020},
