@article{ WOS:000349028700002,
Author = {Yaroshenko, O. P. and Aleksandrova, G. N.},
Title = {Lower Cretaceous palynological assemblages of the Levashi Formation in
   the Aimaki section of Central Dagestan},
Journal = {STRATIGRAPHY AND GEOLOGICAL CORRELATION},
Year = {2015},
Volume = {23},
Number = {1},
Pages = {24-44},
Month = {JAN},
Abstract = {We present the results of the study of higher plant spores and pollen in
   a section of the Levashi Formation near the village of Aimaki. The
   Hauterivian and Barremian palynological associations were defined on the
   basis of changes in the taxonomic content of terrestrial plant spores
   and pollen. The associations are comparable with Hauterivian and
   Barremian assemblages of various regions of the Caucasus with respect to
   the composition of major groups and individual taxa. A transition in the
   vegetation was noted, from the coniferous palynological association of
   the Hauterivian to a fern association in the Barremian. The predominance
   of hygrophilous ferns and the presence of lycophytes and representatives
   of Taxodiaceae and Sphagnopsida in the Barremian (which were nearly
   absent in the Hauterivian), accompanied by a significant decrease in
   thermophilic Cheirolepidiaceae and other conifers, indicate a change in
   climate characterized by an increase in humidity and a decrease in
   temperature.},
