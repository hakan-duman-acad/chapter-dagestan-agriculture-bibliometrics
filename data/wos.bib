Author = {Gavrilov, Yu. O. and Shcherbinina, E. A. and Shchepetova, E. V. and
   Golovanova, O. V. and Pokrovsky, B. G.},
Title = {Cretaceous-Paleogene Boundary in the Sequences of the Northeastern
   Caucasus, Dagestan: Sedimentology, Geochemistry, and Biota},
Journal = {LITHOLOGY AND MINERAL RESOURCES},
