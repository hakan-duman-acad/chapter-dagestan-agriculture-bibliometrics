@article{ WOS:000887073500019,
Author = {Nemtinov, V. I. and Kostanchuk, Y. N. and Motyleva, S. M. and Pekhova,
   O. A. and Timasheva, L. A. and Pashtetskiy, V. S. and Katskaya, A. G.},
Title = {MORPHOMETRIC AND BIOCHEMICAL ASSESSMENT OF<i> NIGELLA</i> L. GENOTYPES
   OF EUROPEAN-ASIAN ORIGIN},
Journal = {SABRAO JOURNAL OF BREEDING AND GENETICS},
