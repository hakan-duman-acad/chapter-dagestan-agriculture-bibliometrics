@article{ WOS:000457398400018,
Author = {Maslov, V. E. and Derzhavin, V. L.},
Title = {A Rich Burial of the Great Migration Period from Northern Dagestan},
Journal = {STRATUM PLUS},
Year = {2018},
Number = {4},
Pages = {333-346},
Abstract = {In 1988, a rich burial was discovered in the catacomb under the tumulus
   at the Yuzbash-Sulak-II cemetery in Northern Dagestan. Gold adornments
   and ceramic analogies date this complex within the period D2 of the
   European chronology - 380/400-440/450 AD. The finds from this elite
   burial demonstrate various vectors of cultural ties: Bosporan Kingdom,
   Caucasian Albania, Alan Central Caucasus. The burial is one of the Alan
   burials on the northern outskirts of the ``country of Maskutians{''} of
   Armenian written sources, whose power centers were in Southern Dagestan.},
