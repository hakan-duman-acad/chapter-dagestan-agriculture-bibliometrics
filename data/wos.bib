@article{ WOS:000348894100009,
Author = {Babayev, E. A. and Magomedov, M-R. D.},
Title = {SELECTION OF BED-SITES BY THE ROE DEER (<i>CAPREOLUS CAPREOLUS</i>) IN
   PIEDMONT DAGESTAN DURING SUMMER},
Journal = {ZOOLOGICHESKY ZHURNAL},
Year = {2014},
Volume = {93},
Number = {12},
Pages = {1470-1476},
Month = {DEC},
Abstract = {The main parameters of vegetation responsible for the selection of
   bed-sites by roe deer in the foothill belt of Dagestan were studied. The
   bed-sites are shown to differ significantly from the neighboring random
   sites in the abundance of grasses (24\% vs. 16\%), herbs (13\% vs. 7\%),
   tree and shrub young growth (11\% vs. 4.5\%), the density of understorey
   cover (7.7\% vs. 3.6\%) and the total projective coverage of plants
   (42.2\% vs. 26.9\%). The mean values of the minimum distance, at which
   the complete hiding of bed-sites took place was almost twice lower (25.7
   m) than at the neighboring randomly chosen sites (44.6) 100 m away. The
   hiding degree of the bed-sites from the fixed 5 and 30 m was also much
   higher than at random sites (20 and 93\% vs. 7.5 and 71\%,
   respectively). In piedmont Dagestan, the bed-site selection by roe-deer
   was not related to foraging and the thermoregulatory function of the
   plant cover, but it was entirely determined by its hiding features. Roe
   deer prefers bedding on flat sites with relatively constant parameters
   of the tree canopy that determine the optimum understorey cover for
   hiding.},
