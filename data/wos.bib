@article{ WOS:A1993KT21200011,
Author = {MAGOMEDOV, MRD and AKHTAYEV, MKR},
Title = {DEPENDENCE OF FEEDING-HABITS AND STATE OF POPULATIONS OF
   MERIONES-TAMARISCINUS ON THE DYNAMICS OF FOOD RESOURCES},
Journal = {ZOOLOGICHESKY ZHURNAL},
Year = {1993},
Volume = {72},
Number = {2},
Pages = {101-111},
Month = {FEB},
Abstract = {An amount of energy obtained by Meriones tamariscinus in nature
   fluctuates in seasons and in years and is determined by natural changes
   of calorific value of the consumed fodder and by the changes of the real
   value (the mass) of its daily consumption. The amount of a fodder
   consumed depends on the crop (r = 0.72-0.92; p < 0.001) and varies in
   different seasons and years from 4.0 to 9.9 gr dry weight per day.
   Nutritiousness of the consumed fodder also changes much with the season
   (1658-1900 kj/100 gr of dried substance) according to the change of the
   nutritiousness of the pasture vegetation (1583-1750 kj). Absolute values
   of the daily energy consumption vary in parallel from 70 to 140 kj per
   individual. Depending on the concrete season conditions and on the state
   of food resources, the energy consumption is lower (summer, winter) or
   higher (spring, autumn) than daily requirements of animals (96-105 kj
   for a grown up non-reproducing individual). Disparity between the needs
   and the consumption causes the decrease (in summer and winter) or
   increase (in spring and autumn) of the body weight which reflects the
   physiological conditions of individuals and, as a result, effects the
   state (reproduction rate, death rate and numbers) of the whole
   population.},
