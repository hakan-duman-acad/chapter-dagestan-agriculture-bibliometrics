@article{ WOS:001223096000003,
Author = {Mikailov, M. M. and Gunashev, Sh. A. and Yanikova, E. A. and Halikov, A.
   A. and Bulashev, Aitbay K.},
Title = {Indirect hemagglutination assay for diagnosing brucellosis: Past,
   present, and future},
Journal = {VETERINARY WORLD},
Year = {2024},
