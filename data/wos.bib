@article{ WOS:000227511200008,
Author = {Guseinov, MK and Osmanov, MM and Guseinov, KM},
Title = {Characteristics in the pelagic ecosystem of the Dagestan region of the
   Caspian Sea under the impact of the ctenophore <i>Mnemiopsis leidyi</i>
   (<i>A-agassiz</i>)},
Journal = {OCEANOLOGY},
Year = {2005},
Volume = {45},
Number = {1},
Pages = {62-65},
Month = {JAN-FEB},
Abstract = {The materials on the condition of the pelagic ecosystem of the Dagestan
   region of the Caspian Sea after the invasion of the ctenophore
   mnemiopsis are presented. It is shown that the mass development of the
   ctenophore occurs in the summer period. It is noted that the
   environmental changes related to the ctenophore invasion produced no
   significant influence on the qualitative composition of the spring
   zooplankton but resulted in a very sharp reduction of its biomass. It is
   found that the abundance of settled juvenile mollusks may strongly
   decrease under the impact of the predatory invader; on this basis, a
   suggestion on the possible reduction of the mollusk biomass is posed.},
