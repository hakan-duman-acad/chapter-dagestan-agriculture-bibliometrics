@article{ WOS:000330753400009,
Author = {Magomaev, M. F. and Avakyan, G. N. and Stakhovskaya, L. V.},
Title = {The dynamics of the morbidity rate of stroke in the Dagestan Republic},
Journal = {ZHURNAL NEVROLOGII I PSIKHIATRII IMENI S S KORSAKOVA},
Year = {2013},
Volume = {113},
Number = {12, 2},
Pages = {42-46},
Abstract = {Results of the first epidemiologic survey of cerebral stroke in the
   Dagestan Republic (DR) carried out in 2009-2010 are presented. Using a
   population register, we evaluated the dynamics of the morbidity rate of
   stroke and effectiveness of primary and secondary prevention of stroke.
   There were 7533 cases of stroke, including 4116 in 2009 and 3413 in
   2010. The morbidity rate per 1000 people was 2.76 in 2009 and 2.29 in
   2010. The diagnoses were distributed as follows: ischemic stroke 3029
   (73.6\%) in 2009 and 2457(72\%) in 2010; hemorrhagic stroke-760 (18.5\%)
   in 2009 and 692 (20.3\%) in 2010; undifferentiated -327 (7.9\%) in 2009
   and 264 (7.7\%) in 2010. Mean age at disease onset was 65.41 +/- 11.4 in
   2009 and 66.43+12.6 in 2010. The measures for primary and secondary
   prevention of stroke performed in DR led to the reduction of stroke
   cases by 17\% in 2010 compared to 2009.},
