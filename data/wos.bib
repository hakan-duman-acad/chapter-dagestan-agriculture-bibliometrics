@article{ WOS:000417631700007,
Author = {Mirzaev, Karin B. and Sychev, Dmitry A. and Ryzhikova, Kristina A. and
   Konova, Olga D. and Mammaev, Suleiman N. and Gafurov, Daniyal M. and
   Shuev, Grigorij N. and Grishina, Elena A. and Sozaeva, Zhannet A.},
Title = {Genetic Polymorphisms of Cytochrome P450 Enzymes and Transport Proteins
   in a Russian Population and Three Ethnic Groups of Dagestan},
Journal = {GENETIC TESTING AND MOLECULAR BIOMARKERS},
Year = {2017},
Volume = {21},
Number = {12},
Pages = {747-753},
Month = {DEC},
Abstract = {Aim: The objective of this study was to investigate the prevalence of
   polymorphic markers of the CYP2C19, CYP2C9, CYP2D6, SLCO1B1, and ABCB1
   genes among the three ethnic groups in Dagestan and compare it with the
   carrier frequency of these markers among the Russian population living
   in Moscow. Methods: The study involved 186 healthy, unrelated, and
   chronic medication-free volunteers (53 males and 133 females) of the
   three ethnic groups in the Dagestan Republic: 46 Laks, 90 Avars, and 50
   Dargins. Genotyping was performed using real-time polymerase chain
   reaction-based methods. The allelic prevalences of the three Dagestan
   peoples were compared with ethnic Russians from the Moscow region.
   Results: Statistically significant differences for the following gene
