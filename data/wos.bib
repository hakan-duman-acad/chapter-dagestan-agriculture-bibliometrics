Editor = {KarimSultanovich, BD},
Title = {NEOPROTESTANTS IN DAGESTAN: VALUE ORIENTATIONS IN POST-SOVIET MISSIONARY
   PRACTICES},
Booktitle = {SOCIAL AND CULTURAL TRANSFORMATIONS IN THE CONTEXT OF MODERN GLOBALISM
   (SCTCGM 2018)},
