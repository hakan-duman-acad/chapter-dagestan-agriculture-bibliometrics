@inproceedings{ WOS:000472156000020,
Author = {Basaeva, E. K. and Kamenetsky, E. S. and Khosaeva, Z. Kh.},
Editor = {KarimSultanovich, BD},
Title = {TERRORIST ACTIVITY IN DAGESTAN AT BEGINNING OF 21ST CENTURY},
Booktitle = {SOCIAL AND CULTURAL TRANSFORMATIONS IN THE CONTEXT OF MODERN GLOBALISM
   (SCTCGM 2018)},
