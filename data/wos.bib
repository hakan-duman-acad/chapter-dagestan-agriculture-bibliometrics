@article{ WOS:000383194300005,
Author = {Seferbekov, Magomedhabib},
Title = {Notes on Mountain Cults in Dagestan},
Journal = {IRAN AND THE CAUCASUS},
Year = {2016},
Volume = {20},
Number = {2},
Pages = {215-218},
Abstract = {As a sacral space, mountains in Dagestan are traditionally considered to
   be the abode of gods and demons. Rainmaking rituals and folk medical
   procedures are performed on the mountain sites. On the tops of
   mountains, their slopes and passes there are many buildings transformed
   later into Muslim places of worship. The paper describes some cases of
   orolatry among the peoples of Dagestan, based on the relevant sources
   and personal field materials.},
