Title = {Dynamics of the growth processes of <i>Prunus armeniaca</i> L. seedlings
   in the conditions of Inner Mountain Dagestan},
Booktitle = {INTERNATIONAL SCIENTIFIC ONLINE-CONFERENCE BIOENGINEERING IN THE
