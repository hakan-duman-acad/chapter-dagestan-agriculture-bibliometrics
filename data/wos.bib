Author = {Mukhtarova, Asiyat M. and Bratkov, Vitalij V. and Biktimirova, Nailya M.
   and Mukhtarova, Gulnara M. and Bekshokova, Patimat A. and Nakhibasheva,
   Gyulnara M.},
Title = {MAPPING OF AREAS OF MODEL SPECIES OF ANIMAL POPULATION OF THE REPUBLIC
   OF DAGESTAN},
