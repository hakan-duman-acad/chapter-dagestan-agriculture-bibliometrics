@article{ WOS:000235061100015,
Author = {Zhukova, OV and Schneider, YV and Morozova, IY and Shilnikova, IN and
   Semikov, AV and Goldenkova, IV and Sarkisova, MK and Radjabov, MO and
   Markarova, EV and Piruzian, AL},
Title = {Genetic markers and psoriasis in three ethnic populations of Dagestan},
Journal = {RUSSIAN JOURNAL OF GENETICS},
Year = {2005},
Volume = {41},
Number = {12},
Pages = {1414-1418},
Month = {DEC},
Abstract = {Analysis of clinical material obtained from the individuals (49
   psoriasis patients and 357 individuals without this disease)
   representing three ethnic populations of Dagestan (Avars, Dargins, and
   Kumyks) was performed. Polymorphism of the blood group loci ABO, Rhesus
   (RH), Kell, P, and Lewis, as well as of the protein-encoding loci for
   haptoglobin (HP), transferrin (TF), the third complement component
   (C'3), group-specific component (GC), and the enzymes, including
   glycosylase (GLO1), esterase D (ESD), 6-phosphate dehydrogenase (6PDG),
   and acid phosphatase (ACP), was studied. It was demonstrated that in the
   pooled sample of Avars and Kumyks the Lewis system phenotype Le(a-b-)
   and the phenotype ee of the Rhesus system were statistically
   significantly more frequent among the psoriasis patients (P = 0.0488 and
   P = 0.0166, respectively), than among healthy controls of the same
   ethnic groups. It was suggested that for the pooled sample of Avars and
   Kumyks, homozygosity for the recessive ee (RH) in combination with the
   Le(a-b-) phenotype, representing homozygosity for recessive allele le,
   was the risk factor for the development of psoriasis.},
