@article{ WOS:000794567700012,
Author = {Aliev, G. B. and Murtazaev, O. A. and Musayeva, I. S.},
Title = {German scientist Adam Olearius on Dagestan in the XVII century},
Journal = {VOPROSY ISTORII},
Year = {2022},
Volume = {4},
Number = {2},
Pages = {143-154},
Abstract = {The article is devoted to the historiographical analysis of the
   information contained in the work of the German scientist Adam Olearius:
   ``Description of the journey to Muscovy and through Muscovy to Persia
   and back{''}, which characterize various aspects of the life of the
   peoples of Dagestan in the 30s of the 17th century. The analysis of the
   material shows that the information contained in his work needs careful
   study and interpretation, since many of his postulates do not correspond
   to reality. The authors come to the conclusion that despite the
   inaccuracies, the objectivity of many information on various issues of
   the history of Dagestan, the work of A. Olearius as a whole is of great
   interest to scientists studying the history of the Dagestan Middle Ages.},
Publisher = {VOPROSY ISTORII},
