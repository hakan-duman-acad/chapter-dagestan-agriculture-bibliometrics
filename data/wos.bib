@article{ WOS:A1996WG16200009,
Author = {Akhmedov, EG and Magomedov, MRD},
Title = {Spatial and temporal structure of daghestan tur (Capra cylindricornis)
   population in the eastern Caucasus and factors responsible for their
   pattern},
Journal = {ZOOLOGICHESKY ZHURNAL},
Year = {1996},
Volume = {75},
Number = {12},
Pages = {1863-1872},
Month = {DEC},
Abstract = {In summer females of Capra cylindricornis occur mostly on northern
   slopes of 36 degrees steep in the alpine and subalpine belts, where
   protection and fodder stock are optimal. They don't perform daily
   movements. Males demonstrate no preferences regarding the exposition of
   mountain slopes, the range of slope steepness is much wider than for
   females. They are characterized by regular and pronounced daily
   movements within the alpine and subnival belts, Different requirements
   of adult males and breeding females for food as well as various
   responses to changes of environmental parameters (food, predators,
   temperature) are suggested to be the main factors determining the
   considered pattern of utilizing the space by turs of different sex and
   age.},
