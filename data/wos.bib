@article{ WOS:A1996VQ91700015,
Author = {Shchipanov, NA and Kasatkin, MV},
Title = {Microtus socialis in changes landscapes of south Dagestan: Population
   aspects of survival},
Journal = {ZOOLOGICHESKY ZHURNAL},
Year = {1996},
Volume = {75},
Number = {9},
Pages = {1412-1426},
Month = {SEP},
Abstract = {Peculiarities of population biology of Microtus socialis are considered.
   The low ability to restoring their numbers under direct destruction is
   shown. The life strategy of the vole is use of constant settlements
   composed of the system of complex burrows. If direct anthropogenous
   effects (deep ploughing, floodings in watering), leading to the death of
   animals, are repeated annually, voles leave their constant settlements.
   The sustainable existence of the species in the disturbed landscapes is
   possible in large territories in the cases of not so permanent intensive
   human activity. These areas are formed in the places where forests
   dissapear due to aridization.},
