Title = {Rhizobia Isolated from the Relict Legume <i>Vavilovia formosa</i>
   Represent a Genetically Specific Group within <i>Rhizobium
   leguminosarum</i> biovar <i>viciae</i>},
Journal = {GENES},
Year = {2019},
