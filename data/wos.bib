Author = {Zarema, Nazirovna Gadgieva},
Editor = {KarimSultanovich, BD},
Title = {INDIVIDUAL HOUSEHOLD PLOTS IN AGRICULTURAL PRODUCTION IN SOUTHERN
   DAGESTAN (1965-1985)},
Booktitle = {SOCIAL AND CULTURAL TRANSFORMATIONS IN THE CONTEXT OF MODERN GLOBALISM
   (SCTCGM 2018)},
