@article{ WOS:A1995QW97300012,
Author = {MAGOMEDOV, MRD and OMAROV, KZ},
Title = {FEEDING-BEHAVIOR AND THE STATE OF POPULATIONS OF CISCAUCASIAN HAMSTER
   (MESOCRICETUS-RADDEI-AVARICUS) IN MOUNTAIN AGROECOSYSTEMS},
Journal = {ZOOLOGICHESKY ZHURNAL},
Year = {1995},
Volume = {74},
Number = {3},
Pages = {123-133},
Month = {MAR},
Abstract = {The feeding levels and the consumption of nutrients and energy by
   Ciscaucasian hamster reach their maximum in agroecosystems with abundant
   grain foods. These parameters, are stable between years;and seasons and
   do not depend on the dynamics of the crops and nutritiousness of natural
   foods (21 to 22g of dry weight and 279 to 309kJ per individual a day
   resp.). The absolute values of daily energy consumption exceed daily
   requirement (206 +/- 16.5 kJ per individual) almost in all periods of
   activity and cause regular increase of body weight throughout entire
   period of activity. This determines the maximum level of reproduction
   (89-98\%), the minimum level of winter mortality (22-54\%) and the
   highest and stable density for years (34-35 animals per ha). In the
   absence of grain grain foods, feeding levels and consumption of
   nutrients are determined by the dynamics of natural foods. Seasonal and
   annual dynamics of these indices cause significant fluctuations of
   reproductive intensity, mortality and migration between years. Even in
   optimum natural biotopes, the population density does not exceed 3 or 5
   animals per ha and varies significantly between years.},
