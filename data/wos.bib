@article{ WOS:A1995RQ81100011,
Author = {HESKE, EJ and SHENBROT, GI and ROGOVIN, KA},
Title = {SPATIAL-ORGANIZATION OF STYLODIPUS-TELUM (DIPODIDAE, RODENTIA) IN
   DAGESTAN, RUSSIA},
Journal = {JOURNAL OF MAMMALOGY},
Year = {1995},
Volume = {76},
Number = {3},
Pages = {800-808},
Month = {AUG},
Abstract = {We conducted a radiotelemetric study of home-range area and overlap of
   an herbivorous jerboa (Stylodipus telum) during its spring breeding
   season in northern Dagestan, Russia. Home ranges of males and females
   were not significantly different in size. There was little intrasexual
   overlap in use of space, particularly among females, but home ranges of
   males and females overlapped extensively. Ranges of any particular male
   or female typically overlapped those of two to four individuals of the
   opposite sex, suggesting a promiscuous or polygynous mating system.
   Individuals used a single day burrow and several shelter burrows. Use of
   day burrows was exclusive, but shelter burrows often were used by
   several individuals. When foraging, S. telum used a walking gait and
   harvested seedheads from low (15-25 cm) grasses; bipedal saltation was
   used only for escape. No food cache or caching behavior was observed.},
