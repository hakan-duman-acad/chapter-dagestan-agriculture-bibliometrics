@article{ WOS:000485016100001,
Author = {Phipps, W. Louis and Lopez-Lopez, Pascual and Buechley, Evan R. and
   Oppel, Steffen and Alvarez, Ernesto and Arkumarev, Volen and
   Bekmansurov, Rinur and Berger-Tal, Oded and Bermejo, Ana and Bounas,
   Anastasios and Carbonell Alanis, Isidoro and de la Puente, Javier and
