@article{ WOS:000278518200011,
Author = {Babayev, E. A. and Magomedov, M-R. D. and Akhmedov, E. G. and Yarovenko,
   Y. A.},
Title = {SPECIFIC FEATURES OF THE WILD BOAR (<i>SUS SCROFA</i>) POPULATION
   STRUCTURE IN PIEDMONT DAGHESTAN},
Journal = {ZOOLOGICHESKY ZHURNAL},
Year = {2010},
Volume = {89},
Number = {3},
Pages = {371-377},
Month = {MAR},
Abstract = {The sex, age, and social structure and character of distributing the bed
   sites in wild boar were investigated in piedmont Daghestan. The most
   numerous group is juveniles (50.0\%); the participation of other groups
   is approximately equal: subadults amounted to 17.6, male adults 16.8,
   and female adults 15.6\%. The sex ratio was 1.00: 1.08; 58.06\% of the
   females breed; the average fertility is 4.92 +/- 0.30 ind. Seven types
   of wild boar social groups were distinguished. Females with juveniles
   were most often registered. The index of wild boar gregarious instinct
   is 5.7 +/- 0.3 ind. The most numerous groups are females with juveniles
   and subadults. Essential differences in the distribution of wild boar
   bed sites were revealed on the northern and southern slopes depending on
   seasons of year. In piedmont Daghestan, the choice of places for bed
   sites was shown to be determined by social groups rather than by the sex
   structure.},
