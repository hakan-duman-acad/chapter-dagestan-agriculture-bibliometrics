@article{ WOS:000090147700009,
Author = {Akhmedov, EG and Magomedov, MRD},
Title = {Formation of the demographic structure in Daghestan tur (<i>Capra
   cylindricornis</i>) population},
Journal = {ZOOLOGICHESKY ZHURNAL},
Year = {2000},
Volume = {79},
Number = {4},
Pages = {461-470},
Month = {APR},
Abstract = {It is shown that mortality factors variously affect different sexual and
   age groups of turs and that this is responsible for the demographic
   structure of their populations. Primary among these factors are feed
   conditions. The influence of other factors is directly or indirectly
   related to feed resources. This year's brood has the highest specific
   mortality, the lowest one is in animals aged 1 to 3 years. Sexual
   differences in mortality start developing in 3-year-old animals. In
   older males the specific mortality in populations increased regularly;
   in females a low level of mortality remains up to the age of 6-7 years.
   As a result, the sex ratio in populations shifts toward an increasing
   share of females.},
