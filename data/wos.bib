@article{ WOS:000640508800001,
Author = {Amirkhanov, H. A.},
Title = {Evolution of the Lithic Industry in Central Dagestan During the Early
   Pleistocene: The Rising Occurrence of Flake Blanks},
Journal = {ARCHAEOLOGY ETHNOLOGY AND ANTHROPOLOGY OF EURASIA},
Year = {2021},
Volume = {49},
Number = {1},
Pages = {3-8},
Abstract = {Archaeological data from stratified Early Pleistocene sites in Central
   Dagestan are arranged in a direct stratigraphic sequence, making it
   possible to reconstruct the changes in lithic industry over a span of
   1.2 min years, from similar to 2.0 to 0.8 Ma BR and to separate the
   principal stages in the Early Paleolithic culture of the Caucasus. This
   study examines blanks found at sites of the Ainikab-Mukhkay group, such
   as Ainikab-1, and Mukhkay-1, -2, and -2a. Occurrences of large fake
   blanks 10 cm) at the Oldowan and the Oldowan to Acheulean transitional
   stage are provided. Such blanks appear at the beginning of the Jaramillo
   paleomagnetic episode (similar to 1.07 Ma BP). By the end of the Early
   Pleistocene, their share attains 25.77 \% of the total number of blanks
   for morphologically distinct tools. They are absent in Oldowan deposits
   (similar to 2 Ma BP). The totality of statistical data justifies the
   separation of the transitional Oldowan to Acheulean stage in the region,
   dating to 1.0-0.8 Ma BR.},
