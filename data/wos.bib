@article{ WOS:A1996UF57200007,
Author = {Caradus, JR and Forde, MB},
Title = {Characterisation of white clover populations collected from the Caucasus
   and high altitude regions of eastern Turkey},
Journal = {GENETIC RESOURCES AND CROP EVOLUTION},
