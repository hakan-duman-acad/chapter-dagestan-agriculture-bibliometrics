@article{ WOS:001049358300009,
Author = {Magomedov, M. Sh. and Magomedova, M. M.},
Title = {Distribution of Trophic Resources in a Rodent Community in the
   High-Mountain Zone of the Republic of Dagestan},
Journal = {RUSSIAN JOURNAL OF ECOLOGY},
