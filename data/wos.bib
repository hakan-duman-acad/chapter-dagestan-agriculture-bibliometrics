   Murtazaliev, Ramazan and Mikic, Aleksandar},
Title = {Reviewing and updating the detected locations of beautiful vavilovia
   (<i>Vavilovia formosa</i>) on the Caucasus sensu stricto},
Journal = {GENETIC RESOURCES AND CROP EVOLUTION},
