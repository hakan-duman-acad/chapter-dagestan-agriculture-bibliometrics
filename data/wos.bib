@article{ WOS:000472562000017,
Author = {Malashev, V. Yu. and Abiev, A. K. and Bakushev, M. A.},
Title = {THE ZELIKAKINSK SETTLEMENT OF THE CAUCASIAN ALBANIA-SARMATIA PERIOD IN
   CASPIAN DAGESTAN},
Journal = {KRATKIE SOOBSHCHENIYA INSTITUTA ARKHEOLOGII},
Year = {2017},
Number = {249, 1},
Pages = {217-230},
Abstract = {The paper describes results of the studies of the Zelikakinsk
   settlement. Midden deposit layers 2,3 m thick found at the edge of the
   settlement as domestic refuse and debris from the area occupied by
   settlement residents were excavated. In addition to that, strata of the
   occupation layer with secondary burials were examined in one section of
   the cemetery. Two components are observed in the ceramic collection,
