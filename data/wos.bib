@article{ WOS:A1993MP69000002,
Author = {ROMASHKEVICH, AI and DAVYDOVA, MV and LOTOV, RA},
Title = {RECENT AND ANCIENT PEDOGENESIS IN SOME LANDSCAPES OF INTERIOR DAGESTAN},
Journal = {EURASIAN SOIL SCIENCE},
Year = {1993},
Volume = {25},
Number = {5},
Pages = {15-27},
Month = {MAY},
Abstract = {Contrasting humid and xeromorphic pedogenesis in the intermediate and
   high mountain areas of interior Dagestan is discussed. It is shown that
   pedogenesis has been shaped by both climate and lithology. Under the
   conditions prevailing in interior Dagestan, the humid series of soils
   are almost exclusively Mountain-Meadow kinds of soils on leached
   sediments underlain by carbonate rocks; the xeromorphic series are
   Mountain-Steppe Soils on eluvium-diluvium of carbonate rocks. The humid
   series has been determined to be polygenetic. Analysis of the structure
   and properties of the soils indicates that their development is tied in
   large measure to natural geomorphological processes. The destructive
   effects of human activity have been exaggerated.},
