@article{ WOS:000425544300010,
Author = {Dudnikov, Alexander Ju},
Title = {Polymorphism of gliadins in <i>Aegilops tauschii</i> Coss. local
   populations in two primary habitats in Dagestan},
Journal = {GENETIC RESOURCES AND CROP EVOLUTION},
Year = {2018},
Volume = {65},
Number = {3},
Pages = {845-854},
Month = {MAR},
Abstract = {Polymorphism of gliadins was investigated in Aegilops tauschii from
   primary habitats ``4{''}, near Hily, and ``6{''}, near Rukel, in
   Dagestan, Russia 205 individual plants were analysed (53/50 and 54/48
   plants of subsp. tauschii/subsp. strangulata from the habitats ``4{''}
   and ``6{''}, respectively) and 1/7 and 18/14 different haplotypes were
   found among the plants of subsp. tauschii/subsp. strangulata from the
   habitats ``4{''} and ``6{''}, respectively. No direct evidences of
   cross-pollination were pointed out, although gliadins electrophoretic
   phenotypes obtained allowed to suggest that it occur in Ae. tauschii
   with very low frequency. The data obtained revealed that during Ae.
   tauschii evolutionary history a local habitat could be populated many
   times by different phylogenetic lineages of the species. It was found
   that in Dagestan, at the very edge of the species area, several
   different lineages belonging to different subspecies could for a long
   time co-exist together in a local habit, and in such case a very high
   level of genetic variation in Ae. tauschii could be accumulated on a
   square of less than one hectare. The further studies of genetic
