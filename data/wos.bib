@article{ WOS:000478171500001,
Author = {Khederzadeh, Saber and Kusza, Szilvia and Huang, Cui-Ping and Markov,
   Nickolay and Scandura, Massimo and Babaev, Elmar and Sprem, Nikica and
   Seryodkin, V, Ivan and Paule, Ladislav and Esmailizadeh, Ali and Xie,
   Hai-Bing and Zhang, Ya-Ping},
Title = {Maternal genomic variability of the wild boar (<i>Sus scrofa</i>)
   reveals the uniqueness of East-Caucasian and Central Italian populations},
Journal = {ECOLOGY AND EVOLUTION},
Year = {2019},
Volume = {9},
Number = {17},
Pages = {9467-9478},
Month = {SEP},
Abstract = {The phylogeography of the European wild boar was mainly determined by
   postglacial recolonization patterns from Mediterranean refugia after the
   last ice age. Here we present the first analysis of SNP polymorphism
   within the complete mtDNA genome of West Russian (n = 8), European (n =
   64), and North African (n = 5) wild boar. Our analyses provided evidence
   of unique lineages in the East-Caucasian (Dagestan) region and in
   Central Italy. A phylogenetic analysis revealed that these lineages are
   basal to the other European mtDNA sequences. We also show close
   connection between the Western Siberian and Eastern European
   populations. Also, the North African samples were clustered with the
   Iberian population. Phylogenetic trees and migration modeling revealed a
   high proximity of Dagestan sequences to those of Central Italy and
   suggested possible gene flow between Western Asia and Southern Europe
   which was not directly related to Northern and Central European
   lineages. Our results support the presence of old maternal lineages in
   two Southern glacial refugia (i.e., Caucasus and the Italian peninsula),
   as a legacy of an ancient wave of colonization of Southern Europe from
   an Eastern origin.},
