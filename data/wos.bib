@article{ WOS:000964662300005,
Author = {Eshchenko, Yu G. and Arakelyan, M. A. and Sukhanov, V. O.},
Title = {Dagestan ASSR in the organization of oil production and transit of
   petroleum products during the Great Patriotic War},
Journal = {VOPROSY ISTORII},
Year = {2023},
Volume = {2},
Number = {2},
Pages = {64-77},
Abstract = {The article examines the importance of the Dagestan ASSR in the
   organization of oil production and transit of petroleum products in the
   south of the country during the Great Patriotic War. The article shows
   the change in the role of the capital and the largest industrial hub of
   the republic - Makhachkala - in oil production during wartime: from a
   major transit point of Baku and Grozny oil, whose cargo turnover
   increased sharply during the initial period of the war, to an actively
   developing oil-producing center. On the basis of archival documents, the
   complex of measures taken by the Soviet command to ensure oil production
   and transit of petroleum products needed by the front in the conditions
   of the loss of part of the oil-producing regions of the Caucasus and the
   Black Sea ports is analyzed.},
